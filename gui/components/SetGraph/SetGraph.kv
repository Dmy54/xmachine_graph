#:import MeshLinePlot kivy.garden.graph.MeshLinePlot

<SetGraph>:
    graph_main : graph_main
    Graph:
        id: graph_main
        plot: MeshLinePlot
        xlabel:'Повторение'
        ylabel:'real and expected force'
        x_ticks_major:10
        y_ticks_major:1
        y_grid_label:True
        x_grid_label:True
        padding:5
        x_grid:True
        y_grid:True
        xmin:-0
        xmax:100
        ymin:-10
        ymax:10
        pos: 0, root.height / 6
        size: root.width * 2 / 3 , root.height * 18 / 24